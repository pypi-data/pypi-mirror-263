{
  "name": "jupyterlab-broccoli",
  "version": "0.4.7",
  "description": "Fork of blockly extension for JupyterLab.",
  "keywords": [
    "blockly",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/FumiHax/jupyterlab-broccoli",
  "bugs": {
    "url": "https://github.com/FumiHax/jupyterlab-broccoli/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Fumi.Iseki",
    "email": "iseki@rsch.tuis.ac.jp"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/FumiHax/jupyterlab-broccoli.git"
  },
  "scripts": {
    "build": "tsc -b",
    "build:prod": "jlpm build",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "jlpm clean:lib",
    "install:extension": "jlpm build"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.9",
    "@jupyterlab/apputils": "^4.1.9",
    "@jupyterlab/cells": "~4.0.9",
    "@jupyterlab/codeeditor": "^4.0.9",
    "@jupyterlab/codemirror": "^4.0.9",
    "@jupyterlab/coreutils": "^6.0.9",
    "@jupyterlab/docregistry": "^4.0.9",
    "@jupyterlab/notebook": "^4.0.9",
    "@jupyterlab/rendermime": "^4.0.9",
    "@jupyterlab/services": "^7.0.9",
    "@jupyterlab/settingregistry": "^4.0.9",
    "@jupyterlab/translation": "^4.0.9",
    "@jupyterlab/ui-components": "^4.0.9",
    "@lumino/algorithm": "^2.0.1",
    "@lumino/coreutils": "^2.1.2",
    "@lumino/messaging": "^2.0.1",
    "@lumino/signaling": "^2.1.2",
    "@lumino/widgets": "^2.3.1",
    "@types/react": "^18.2.41",
    "blockly": "^9.3.3",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "rimraf": "^4.4.0",
    "typescript": "~4.9.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  }
}
